<div class="tpay-preload" style="display:none;">
	<div class="tpay-preload__inner">
		<div class="tpay-preload__loader">
			<svg class="tpay-preload__circular" viewBox="25 25 50 50">
				<circle class="tpay-preload__path" cx="50" cy="50" r="20"
				        fill="none" stroke-width="6" stroke-miterlimit="10"/>
			</svg>
		</div>
		<span class="tpay-preload__content">
			{l s='Processing data, please wait...' mod='tpay'}
		</span>
	</div>
</div>
